{
  "name": "react-markdocs-example",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:**",
    "start": "npm run build && npm-run-all --parallel server watch:**",
    "test": "mocha-phantomjs -s loadImages=false ./www/tests.html",
    "prebuild": "rimraf www && mkdirp www",
    "prestart": "npm run prebuild",
    "build:static": "cpx \"src/static/**/*\" www",
    "build:tests": "cpx \"node_modules/mocha/mocha*\" www/lib",
    "watch:static": "npm run build:static -- -w",
    "build:js": "webpack",
    "watch:js": "npm run build:js -- --watch",
    "build:css": "npm run watch:css -- --no-watch",
    "watch:css": "autoless --source-map src www",
    "server": "live-server www --port=8888"
  },
  "author": "k88hudson",
  "license": "MPL-2.0",
  "dependencies": {
    "lodash.throttle": "^3.0.4",
    "react": "^0.13.3",
    "react-markdocs": "https://github.com/k88hudson/react-markdocs.git",
    "typeset": "https://github.com/k88hudson/Typeset.git"
  },
  "engines": {
    "npm": ">=2.0.0"
  },
  "devDependencies": {
    "autoless": "^0.1.7",
    "babel-core": "^5.2.17",
    "babel-loader": "^5.0.0",
    "cpx": "^1.1.6",
    "inject-loader": "^2.0.1",
    "json-loader": "^0.5.2",
    "jsx-loader": "^0.13.2",
    "live-server": "^0.7.1",
    "mkdirp": "^0.5.0",
    "mocha": "^2.2.5",
    "mocha-phantomjs": "^3.6.0",
    "npm-run-all": "^1.2.4",
    "phantomjs": "^1.9.7-15",
    "phantomjs-polyfill": "0.0.1",
    "rimraf": "^2.3.3",
    "should": "^7.0.4",
    "webpack": "^1.9.4"
  }
}
